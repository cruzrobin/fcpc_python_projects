#By Two
print ("By Two")
for a in range (2,12,2):
    print (a)
#By Three
print ("By Three")
for b in range (3,18,3):
    print (b)
#By Four
print ("By Four")
for c in range (4,24,4):
    print (c)
#By Twelve
print ("By Twelve")
for d in range (12,72,12):
    print (d)

#Multpilication Table
print ("Multiplication Table")
for multi in range(10,21):
    for Table in range (1,21):
     print (multi*Table, end ='\t')
    print()
